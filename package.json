{
	"name": "typeboost.css",
	"version": "0.1.8",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"description": "Typeboost.css is a set of CSS rules to ensure readable, scalable and responsive typography. REMs are used for font sizes, paddings, and margins. It's like typography normalizer. No dependencies. Wrap your HTML content with class .col and you are done.",
	"keywords": [
		"typeboost.css",
		"typeface",
		"font",
		"css",
		"grid",
		"typography"
	],
	"main": "main.js",
	"window": {
		"icon": "favicon-32x32.png"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/englishextra/typeboost.css.git"
	},
	"files": [
		"LICENSE.md",
		"index.html",
		"css/typeboost.css.css",
		"css/typeboost.css.min.css"
	],
	"filename": "css/typeboost.css.min.css",
	"autoupdate": {
		"source": "git",
		"target": "git://github.com/englishextra/typeboost.css.git",
		"fileMap": [
			{
				"basePath": "",
				"files": [
					"css/typeboost.css*"
				]
			}
		]
	},
	"dependencies": {
		"npm": "^7.3.0"
	},
	"devDependencies": {
		"browser-sync": "^2.23.7",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^6.0.0",
		"gulp-clean-css": "^4.0.0",
		"gulp-cli": "^2.3.0",
		"gulp-plumber": "^1.2.1",
		"gulp-prettier": "^2.0.0",
		"gulp-rename": "^1.4.0",
		"gulp-sass": "^4.0.2",
		"gulp-sass-lint": "^1.4.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-strip-debug": "^3.0.0"
	},
	"scripts": {
		"test": "gulp lint-libbundle-css"
	},
	"email": "englishextra@mail.ru",
	"url": "https://github.com/englishextra/typeboost.css/issues",
	"maintainers": [
		{
			"name": "Serguei Shimansky",
			"email": "englishextra@mail.ru",
			"url": "https://github.com/englishextra"
		}
	],
	"author": {
		"name": "englishextra",
		"email": "englishextra@mail.ru",
		"url": "https://github.com/englishextra"
	},
	"license": "(MIT)"
}
